<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.controllers.individualElementsControllers.NewCameraController">
   <children>
      <HBox alignment="BOTTOM_RIGHT" prefHeight="75.0" style="-fx-background-color: #5677fc;">
         <children>
            <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="New Camera">
               <HBox.margin>
                  <Insets bottom="10.0" right="25.0" />
               </HBox.margin>
               <font>
                  <Font size="41.0" />
               </font>
            </Text>
         </children>
         <effect>
            <DropShadow />
         </effect>
      </HBox>
      <VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" stylesheets="@../stylesheets/style.css">
         <children>
            <HBox alignment="CENTER_LEFT">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Zfar:">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Text>
                  <TextField fx:id="zFar" prefHeight="26.0" prefWidth="100.0">
                     <HBox.margin>
                        <Insets left="30.0" />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="50.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Znear:" />
                  <TextField fx:id="zNear" prefHeight="26.0" prefWidth="62.0">
                     <HBox.margin>
                        <Insets left="30.0" />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="547.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Fovy:" />
                  <TextField fx:id="fovY" prefHeight="26.0" prefWidth="75.0">
                     <HBox.margin>
                        <Insets left="30.0" />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="50.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Vup:" />
                  <HBox alignment="CENTER_RIGHT">
                     <children>
                        <TextField fx:id="vupX" prefHeight="26.0" prefWidth="75.0" promptText="X">
                           <HBox.margin>
                              <Insets left="30.0" />
                           </HBox.margin>
                        </TextField>
                        <TextField fx:id="vupY" layoutX="157.0" layoutY="22.0" prefHeight="26.0" prefWidth="75.0" promptText="Y">
                           <HBox.margin>
                              <Insets left="25.0" />
                           </HBox.margin>
                        </TextField>
                        <TextField fx:id="vupZ" layoutX="232.0" layoutY="22.0" prefHeight="26.0" prefWidth="75.0" promptText="Z">
                           <HBox.margin>
                              <Insets left="25.0" />
                           </HBox.margin>
                        </TextField>
                     </children>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </HBox>
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="50.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Center:" />
                  <HBox alignment="CENTER_RIGHT" prefHeight="50.0">
                     <children>
                        <TextField fx:id="centerX" prefHeight="26.0" prefWidth="75.0" promptText="X">
                           <HBox.margin>
                              <Insets left="30.0" />
                           </HBox.margin>
                        </TextField>
                        <TextField fx:id="centerY" layoutX="157.0" layoutY="22.0" prefHeight="26.0" prefWidth="75.0" promptText="Y">
                           <HBox.margin>
                              <Insets left="25.0" />
                           </HBox.margin>
                        </TextField>
                        <TextField fx:id="centerZ" layoutX="232.0" layoutY="22.0" prefHeight="26.0" prefWidth="75.0" promptText="Z">
                           <HBox.margin>
                              <Insets left="25.0" />
                           </HBox.margin>
                        </TextField>
                     </children>
                  </HBox>
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="50.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Eye:" />
                  <HBox alignment="CENTER_RIGHT" prefHeight="50.0">
                     <children>
                        <TextField fx:id="eyeX" prefHeight="26.0" prefWidth="75.0" promptText="X">
                           <HBox.margin>
                              <Insets left="30.0" />
                           </HBox.margin>
                        </TextField>
                        <TextField fx:id="eyeY" layoutX="157.0" layoutY="22.0" prefHeight="26.0" prefWidth="75.0" promptText="Y">
                           <HBox.margin>
                              <Insets left="25.0" />
                           </HBox.margin>
                        </TextField>
                        <TextField fx:id="eyeZ" layoutX="232.0" layoutY="22.0" prefHeight="26.0" prefWidth="75.0" promptText="Z">
                           <HBox.margin>
                              <Insets left="25.0" />
                           </HBox.margin>
                        </TextField>
                     </children>
                  </HBox>
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="50.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Camera Name:" />
                  <TextField fx:id="cameraName" prefHeight="26.0" prefWidth="310.0" promptText="Required">
                     <HBox.margin>
                        <Insets left="15.0" />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="50.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="File Name" />
                  <TextField fx:id="fileName" prefHeight="26.0" prefWidth="339.0" promptText="Required">
                     <HBox.margin>
                        <Insets left="15.0" />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
            <Separator prefWidth="200.0" />
            <HBox alignment="CENTER_RIGHT" prefHeight="50.0">
               <children>
                  <TextField fx:id="outputLocation" prefHeight="26.0" prefWidth="394.0" promptText="File Output Location">
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </TextField>
                  <Button mnemonicParsing="false" onAction="#selectLocation" styleClass="btn" text="...">
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="btnCreateCamera" mnemonicParsing="false" onAction="#createCamera" styleClass="btn" text="Create Camera">
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin></Button>
               </children>
            </HBox>
         </children>
         <padding>
            <Insets left="25.0" right="25.0" top="15.0" />
         </padding>
      </VBox>
   </children>
</VBox>
